<nav>
  <div class="bg-box"></div>
  <div class="bg-fade"></div>
  <div class="inner">
    <ul>
      {% for link in data.links %}
        <li>
          <a href="{{link.url}}">{{link.label}}</a>
        </li>
      {% endfor %}
    </ul>
    {% if data.locale == 'nl' %}
      <a href="/en" class="language en">English</a>
    {% else %}
      <a href="/nl" class="language nl">Nederlands</a>
    {% endif %}
  </div>
</nav>
